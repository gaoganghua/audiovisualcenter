 <% if(data!=null && data.length>0) { %>
	<table class="table table-striped table-bordered table-hover">
		<thead>
			<tr>
				<th>名称</th>
				<th>编号</th>
				<th>数量</th>
	
				<th>
					<i class="icon-time bigger-110"></i>
					状态
				</th>
			</tr>
		</thead>
		<tbody>
			<% for(var i=0;i<data.length;i++){%>
				<tr>
					<td>
						<a href="javascript:void(0)"><%=data[i].equipName%> </a>
					</td>
					<td><%=data[i].equipNo%> </td>
					<td><%=data[i].total%> </td>
					<td>
						<%if(data[i].condition==true){%>
							良好
						<%}else{%>
							损坏
						<%}%>
					</td>
				</tr>
			<%}%>
		</tbody>
<%}else{%>
		<h5 class="modal-title">没有任何记录</h5>
	<br>
<%}%>
